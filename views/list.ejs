<%- include("header") -%>
    <div class="box">

        <h1 id="heading"> <%= listtitle %> </h1>

    </div>
    
    <div class="box">
        <% for(var i = 0 ; i<newlistitems.length ; i++ ){  %>
         <div class="item">
            <input type="checkbox">
            <p><%= newlistitems[i] %></p>

         </div>   
         <%   }  %> 
         <form class="" action="/" method="post">
            <input type = "text" name="newitem" placeholder="New Item" autocapitalize="on">
            <button type="submit" name="list" value = <%= %> >+</button>
        </form>
    </div>
    <%- include("footer") -%>